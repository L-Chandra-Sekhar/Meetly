{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "MONGODB_URI": "mongodb_uri",
    "JWT_SECRET": "jwt_secret",
    "JWT_EXPIRES_IN": "7d",
    "NEXT_PUBLIC_ZEGO_APP_ID": "zego_app_id",
    "NEXT_PUBLIC_ZEGO_SERVER_SECRET": "zego_server_secret",
    "EMAIL_USER": "email_user",
    "EMAIL_PASS": "email_pass",
    "NODE_ENV": "production"
  }
}
